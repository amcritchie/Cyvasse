<h1>Listing game_statuses</h1>

<table>
  <thead>
    <tr>
      <th>P1 piece loc</th>
      <th>P2 piece loc</th>
      <th>Turn</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_statuses.each do |game_status| %>
      <tr>
        <td><%= game_status.p1_piece_loc %></td>
        <td><%= game_status.p2_piece_loc %></td>
        <td><%= game_status.turn %></td>
        <td><%= game_status.status %></td>
        <td><%= link_to 'Show', game_status %></td>
        <td><%= link_to 'Edit', edit_game_status_path(game_status) %></td>
        <td><%= link_to 'Destroy', game_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game status', new_game_status_path %>
